

char * removeOuterParentheses(char * S){
    int i, sum=0, j=0;
    int len = strlen(S);
    for(i=0; i<len; i++)
    {
        if (S[i] == '(')
        {
            sum+= 1;
        }
        else{
            sum-= 1;
        }
        if(S[i] == '(' && sum > 1)
        {
            S[j] = '(';
            j++;
        }
        else if (S[i] == ')' && sum > 0){
            S[j] = ')';
            j++;
        }
    }
    S[j] = '\0';
    return S;
}

